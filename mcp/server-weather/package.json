{
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "description": "Weather MCP server for business-links plugin testing",
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/express": "4.17.23",
    "@types/node": "20.19.9",
    "nodemon": "3.1.10",
    "ts-node": "10.9.2",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "name": "weather-mcp-server",
  "scripts": {
    "build": "tsc",
    "dev": "npx tsx src/index.ts",
    "dev:tsnode": "npx ts-node --esm src/index.ts",
